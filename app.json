{
  "expo": {
    "name": "Nutri Ninja",
    "displayName": "Nutri Ninja",
    "slug": "nutrininja",
    "version": "1.0.7",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "nutrininja",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "bundleIdentifier": "com.vino.nutrininja",
      "supportsTablet": false,
      "usesAppleSignIn": true,
      "buildNumber": "3",
      "infoPlist": {
        "NSCameraUsageDescription": "Nutri Ninja needs camera access to analyze your food and provide accurate nutritional information.",
        "NSPhotoLibraryUsageDescription": "Nutri Ninja needs access to your photo library so you can select food images for nutritional analysis to get detailed nutritional information.",
        "FacebookAppID": "",
        "FacebookClientToken": "",
        "FacebookDisplayName": "Nutri Ninja",
        "LSApplicationQueriesSchemes": [
          "fbapi",
          "fb-messenger-api",
          "fbauth2",
          "fbshareextension"
        ],
        "NSUserTrackingUsageDescription": "This identifier will be used to deliver personalized ads to you and improve your experience.",
        "SKAdNetworkItems": [
          {
            "SKAdNetworkIdentifier": ""
          },
          {
            "SKAdNetworkIdentifier": ""
          }
        ]
      }
    },
    "android": {
      "package": "com.vino.nutrininja"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-apple-authentication",
      "expo-camera",
      "expo-notifications",
      "expo-font",
      [
        "expo-build-properties",
        {
          "android": {
            "minSdkVersion": 26
          }
        }
      ],
      [
        "react-native-appsflyer",
        {
          "shouldUseStrictMode": true
        }
      ],
      [
        "react-native-fbsdk-next",
        {
          "appID": "",
          "clientToken": "",
          "displayName": "Nutri Ninja",
          "advertiserIDCollectionEnabled": true,
          "autoLogAppEventsEnabled": true,
          "isAutoInitEnabled": true,
          "iosUserTrackingPermission": "This identifier will be used to deliver personalized ads to you."
        }
      ],
      "@react-native-google-signin/google-signin"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": ""
      }
    },
    "owner": "besch1",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "enabled": false
    }
  }
}
